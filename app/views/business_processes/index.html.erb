<h1>Business Processes</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Author</th>
      <th>Route</th>
      <th>Executed</th>
      <th>Execution date</th>
      <th>File</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_processes.each do |business_process| %>
      <tr>
        <td><%= business_process.description %></td>
        <td><%= business_process.author_id %></td>
        <td><%= Route.find_by(id: business_process.route_id).title %></td>
        <td><%= business_process.executed %></td>
        <td><%= business_process.execution_date %></td>
        <td><%= business_process.file_id %></td>
        <td><%= link_to 'Show', business_process %></td>
        <td><%= link_to 'Edit', edit_business_process_path(business_process) %></td>
        <td><%= link_to 'Destroy', business_process, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business Process', new_business_process_path %>
